<script data-defer-js-ignore="true" type="text/javascript">
  // Get the current URL
  var currentUrl = window.location.href;

  // Get all anchor elements in the footer
  var footerLinks = document.querySelectorAll(".footer_fix a");

  // Loop through each anchor element
  footerLinks.forEach(function(link) {
    // Check if the href attribute matches the current URL
    if (link.href === currentUrl) {
      // If the link is active, add the 'active' class
      link.classList.add("activeLink");
    } else {
      // If the link is inactive, add the 'inactive' class
      link.classList.add("inactiveLink");
      link.classList.remove("activeLink");
    }
  });

  //   below code is for mobile categories


  const btns = document.querySelectorAll(".btn_cat");
  const navDivs = document.querySelectorAll(".sub_c");

  if (btns.length > 0 && navDivs.length > 0) {
    btns[0].classList.add("cv_act");
    navDivs[0].style.display = "block";
  }

  btns.forEach((links, index) => {
    ['click', 'mouseover'].forEach(event => {
      links.addEventListener(event, (e) => {
        activeToggle(links, index, e);
      });
    });
  });


  function activeToggle(links, activeIt, e) {
    btns.forEach((items) => {
      items.classList.remove("cv_act");
    });
    e.target.classList.add("cv_act");

    navDivs.forEach((divs, index) => {
      divs.style.display = (index === activeIt) ? "block" : "none";
    });
  }


  function back_cat() {
    document.getElementById("sub_cat").style.display = "block";
    document.getElementById("inr_cat").style.display = "none";
    document.getElementById("sub_catmp").style.display = "block";
    document.getElementById("inr_catmp").style.display = "none";
  }
</script>